{
 "namespace": "information",
 "button_information@settings_common.section_toggle_base": {
  "$toggle_view_binding_name": "information_button_toggle",
  "$glyph_texture": "textures/ui/info_glyph",
  "$glyph_color_texture": "textures/ui/info_glyph_color",
  "$button_text": "informationSection.information",
  "$glyph_size": [
   16,
   20
  ],
  "$toggle_property_bag": {
   "#panel_title": "informationSection.information.title"
  }
 },
 "information_tab_section": {
  "type": "stack_panel",
  "orientation": "vertical",
  "anchor_from": "top_left",
  "anchor_to": "top_left",
  "size": [
   "100%",
   "100%c"
  ],
  "bindings": [
   {
    "binding_type": "view",
    "source_control_name": "information_button_toggle",
    "source_property_name": "#toggle_state",
    "target_property_name": "#visible"
   }
  ],
  "controls": [
   {
    "title@common.img": {
     "anchor_from": "center",
     "anchor_to": "center",
     "texture": "textures/ui/title",
     "layer": 2,
     "size": [
      "100%",
      42
     ]
    }
   },
   {
    "space0@common.space": {
     "size": [
      0,
      2
     ]
    }
   },
   {
    "info_creator@settings_common.selector_group_label": {
     "text": "informationSection.infoCreator.title",
     "font_size": "large",
     "shadow": true
    }
   },
   {
    "space1@common.space": {
     "size": [
      0,
      2
     ]
    }
   },
   {
    "my_life@settings_common.selector_group_label": {
     "text": "informationSection.myLife.info",
     "alpha": 0.8,
     "shadow": true
    }
   },
   {
    "padding0@common.space": {
     "size": [
      0,
      6
     ]
    }
   },
   {
    "youtube@settings_common.selector_group_label": {
     "text": "informationSection.youtube",
     "shadow": true
    }
   },
   {
    "space2@common.space": {
     "size": [
      0,
      2
     ]
    }
   },
   {
    "discord@settings_common.selector_group_label": {
     "text": "informationSection.discord",
     "shadow": true
    }
   },
   {
    "space3@common.space": {
     "size": [
      0,
      2
     ]
    }
   },
   {
    "terms_of_use@settings_common.selector_group_label": {
     "text": "informationSection.termsOfUse.title",
     "font_size": "large",
     "shadow": true,
     "alpha": 0.50
    }
   },
   {
    "space4@common.space": {
     "size": [
      0,
      2
     ]
    }
   },
   {
    "terms_of_use_youtubers@settings_common.selector_group_label": {
     "text": "informationSection.termsOfUse.youtubers",
     "font_size": "medium",
     "shadow": true,
     "alpha": 0.50
    }
   },
   {
    "space5@common.space": {
     "size": [
      0,
      4
     ]
    }
   },
   {
    "terms_of_use_creators@settings_common.selector_group_label": {
     "text": "informationSection.termsOfUse.creators",
     "font_size": "medium",
     "shadow": true,
     "alpha": 0.50
    }
   }
  ]
 }
}