{
  "namespace": "progress",
  "loading_resources_panel@common.img": {
    "texture": "textures/ui/black_content",
    "alpha": 0.6,
    "size": [
      "100%c+4px",
      "100%c+4px"
    ],
    "controls": [
      {
        "loading_resources_label@progrees.loading_resources_label": {}
      }
    ]
  },
  "generating_terrain_panel@common.img": {
    "texture": "textures/ui/black_content",
    "alpha": 0.6,
    "size": [
      324,
      58
    ],
    "controls": [
      {
        "stack_panel_content@common.vertical": {
          "size": [
            "100%-4px",
            "100%-4px"
          ],
          "controls": [
            {
              "generating_terrain_label@progress.generating_terrain_label": {}
            },
            {
              "please_wait_label@progress.please_wait_label": {}
            }
          ]
        }
      }
    ]
  },
  "generating_terrain_label@common.txtTen": {
    "text": "progressScreen.generatingTerrain.title",
    "text_alignment": "center",
    "font_scale_factor": 1.4,
    "size": [
      "default",
      16
    ]
  },
  "please_wait_label@common.txt": {
    "text": "progressScreen.pleaseWaitMoment",
    "text_alignment": "center",
    "shadow": true,
    "size": [
      "default",
      10
    ]
  },
  "loading_resources_label@common.txtTen": {
    "text": "progressScreen.loadingResources.title",
    "text_alignment": "center",
    "font_scale_factor": 1.4,
    "size": [
      "default",
      16
    ]
  },
  "ios_mobile_data_icon@common.img": {
    "texture": "textures/ui/ios_mobile_data_icon",
    "size": [
      18,
      18
    ]
  },
  "android_mobile_data_icon@common.img": {
    "texture": "textures/ui/android_mobile_data_icon",
    "size": [
      18,
      18
    ]
  },
  "mobile_data_label@common.txt": {
    "text": "progress.mobileDataText",
    "color": "$mobile_data_icon_text_color",
    "shadow": true,
    "size": [
      "default",
      12
    ]
  },
  "mobile_data_active_label@common.txt": {
    "text": "progress.mobileDataActiveText",
    "color": "$mobile_data_icon_text_color",
    "shadow": true,
    "size": [
      "default",
      12
    ]
  },
  "save_icon": {
    "type": "image",
    "layer": 31,
    "texture": "textures/ui/auto_save",
    "anchor_from": "center",
    "anchor_to": "center",
    "uv": "@progress.save_animation_icon",
    "disable_anim_fast_forward": true,
    "size": [
      18,
      30
    ],
    "uv_size": [
      18,
      30
    ],
    "bindings": [
      {
        "binding_name": "#auto_save_animation_visible",
        "binding_name_override": "#visible"
      }
    ]
  },
  "save_animation_icon": {
    "anim_type": "flip_book",
    "frame_count": 9,
    "frame_step": 18,
    "fps": 10,
    "reversible": true,
    "easing": "linear",
    "initial_uv": [
      0,
      0
    ]
  },
  "mobile_data_panel": {
    "type": "image",
    "texture": "textures/ui/black_content",
    "alpha": 0.6,
    "ignored": "(not ($is_android or $is_ios))",
    "layer": -6,
    "anchor_from": "top_right",
    "anchor_to": "top_right",
    "offset": [
      -4,
      4
    ],
    "size": [
      "14%+4px",
      28
    ],
    "controls": [
      {
        "icon_android@progress.android_mobile_data_icon": {
          "ignored": "(not $is_android)",
          "anchor_to": "left_middle",
          "anchor_from": "left_middle",
          "size": [
            24,
            24
          ],
          "offset": [
            2,
            0
          ]
        }
      },
      {
        "icon_ios@progress.ios_mobile_data_icon": {
          "ignored": "(not $is_ios)",
          "anchor_to": "left_middle",
          "anchor_from": "left_middle",
          "size": [
            24,
            24
          ],
          "offset": [
            2,
            0
          ]
        }
      },
      {
        "text_panel": {
          "type": "stack_panel",
          "orientation": "vertical",
          "anchor_to": "left_middle",
          "anchor_from": "left_middle",
          "offset": [
            26,
            0
          ],
          "size": [
            "100%",
            24
          ],
          "controls": [
            {
              "mobile_data@progress.mobile_data_label": {}
            },
            {
              "active_mobile_data@progress.mobile_data_active_label": {}
            }
          ]
        }
      }
    ],
    "bindings": [
      {
        "binding_name": "#mobile_data_icon_visible",
        "binding_name_override": "#visible",
        "binding_condition": "once"
      }
    ]
  },
  "world_save_modal_progress_screen_content": {
    "type": "panel",
    "anchor_from": "center",
    "anchor_to": "center",
    "size": [
      "100%",
      "100%"
    ],
    "controls": [
      {}
    ]
  },
  "world_convert_modal_progress_screen_content": {
    "type": "panel",
    "layer": 3,
    "size": [
      "100%",
      "100%"
    ],
    "controls": [
      {
        "mobile_data_icon@progress.mobile_data_panel": {}
      },
      {
        "title_panel_content@common_art.title_panel_content": {
          "$splash_text_visible": false,
          "layer": -2
        }
      },
      {
        "world_modal_progress_panel@progress.world_modal_progress_panel": {
          "$modal_button_panel_type": "progress.modal_button_panel"
        }
      },
      {
        "popup_dialog_factory@progress.popup_dialog_factory": {}
      }
    ]
  },
  "progress_screen@common.base_screen": {
    "force_render_below": "$force_render_below",
    "$screen_animations": [
      "@common.screen_exit_animation_push_fade",
      "@common.screen_exit_animation_pop_fade"
    ],
    "button_mappings": [
      {
        "from_button_id": "button.menu_cancel",
        "to_button_id": "button.menu_exit",
        "mapping_type": "global"
      },
      {
        "from_button_id": "button.menu_exit",
        "to_button_id": "button.menu_cancel",
        "mapping_type": "global"
      }
    ]
  }
}