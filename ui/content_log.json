{
 "namespace": "content_log",
 "content_log_label": {
  "type": "label",
  "layer": 32,
  "text": "#text",
  "font_type": "smooth",
  "size": [
   "100%-2px",
   "default"
  ],
  "shadow": true,
  "localize": false,
  "color": "$chat_text_color",
  "anims": [
   "@content_log.anim_content_log_txt_wait"
  ],
  "bindings": [
   {
    "binding_name": "#content_log_text",
    "binding_name_override": "#text",
    "binding_type": "collection",
    "binding_collection_name": "content_log_text_grid",
    "binding_condition": "once"
   }
  ]
 },
 "content_log_grid_item": {
  "type": "panel",
  "layer": 1,
  "anchor_from": "top_left",
  "anchor_to": "top_left",
  "size": [
   "100%",
   "100%c"
  ],
  "controls": [
   {
    "content_log_background": {
     "type": "image",
     "texture": "textures/ui/black_content",
     "alpha": 0.8,
     "size": [
      "100%+4px",
      "100%c+2px"
     ],
     "anims": [
      "@content_log.anim_content_log_bg_wait"
     ],
     "controls": [
      {
       "content_log_txt@content_log_label": {
        "anchor_from": "top_left",
        "anchor_to": "top_left",
        "offset": [
         2,
         2
        ]
       }
      }
     ]
    }
   }
  ]
 },
 "content_log_panel": {
  "type": "panel",
  "anchor_from": "top_left",
  "anchor_to": "top_left",
  "size": [
   "100%",
   "100%c"
  ],
  "max_size": [
   "60%",
   "40%"
  ],
  "controls": [
   {
    "stack_panel": {
     "type": "stack_panel",
     "anchor_from": "bottom_left",
     "anchor_to": "bottom_left",
     "factory": {
      "name": "content_log_item_factory",
      "max_children_size": 50,
      "control_ids": {
       "content_log_item": "content_log_item@content_log.content_log_grid_item"
      }
     }
    }
   }
  ]
 }
}