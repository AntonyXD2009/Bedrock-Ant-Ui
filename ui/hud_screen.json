{
 "namespace": "hud",
 "position_panel": {
  "type": "image",
  "texture": "textures/ui/black_content",
  "alpha": 0.6,
  "layer": 1,
  "size": [
   "100%c+4px",
   "100%c+4px"
  ],
  "controls": [
   {
    "position_text": {
     "type": "label",
     "enable_profanity_filter": false,
     "text": "#text",
     "shadow": true,
     "layer": 2,
     "bindings": [
      {
       "binding_name": "#player_position_text",
       "binding_condition": "always_when_visible",
       "binding_name_override": "#text",
       "binding_type": "global"
      }
     ]
    }
   }
  ],
  "bindings": [
   {
    "binding_name": "#player_position_visible",
    "binding_name_override": "#visible",
    "binding_condition": "always",
    "binding_type": "global"
   }
  ]
 },
 "cursor_renderer": {
  "type": "custom",
  "renderer": "cursor_renderer",
  "layer": 4,
  "size": [
   16,
   16
  ]
 },
 "mob_effects_renderer": {
  "type": "custom",
  "renderer": "mob_effects_renderer",
  "layer": 2,
  "size": [
   12,
   12
  ]
 },
 "chat_panel@common.panel": {
  "anchor_from": "top_left",
  "anchor_to": "top_left",
  "size": [
   "100%",
   "100%c"
  ],
  "max_size": [
   "100%",
   "50%"
  ],
  "controls": [
   {
    "stack_panel": {
     "type": "stack_panel",
     "anchor_from": "bottom_left",
     "anchor_to": "bottom_left",
     "factory": {
      "name": "chat_item_factory",
      "max_children_size": 50,
      "control_ids": {
       "chat_item": "chat_item@hud.chat_grid_item"
      }
     }
    }
   }
  ]
 },
 "chat_grid_item@common.panel": {
  "anchor_from": "top_left",
  "anchor_to": "top_left",
  "layer": 1,
  "size": [
   "100%+2px",
   "100%c"
  ],
  "bindings": [
   {
    "binding_name": "(not #on_new_death_screen)",
    "binding_name_override": "#visible"
   }
  ],
  "controls": [
   {
    "chat_background": {
     "type": "image",
     "texture": "textures/ui/black_content",
     "alpha": 0.5,
     "size": [
      "100%",
      "100%c"
     ],
     "anims": [
      "@hud.anim_chat_bg_wait"
     ],
     "controls": [
      {
       "chat_text@chat_label": {
        "anchor_from": "top_left",
        "anchor_to": "top_left",
        "offset": [
         4,
         0
        ]
       }
      }
     ]
    }
   }
  ]
 },
 "anim_chat_bg_offset": {
  "anim_type": "offset",
  "easing": "in_quart",
  "destroy_at_end": "chat_grid_item",
  "duration": 1,
  "from": [
   0,
   0
  ],
  "to": [
   "-50%",
   0
  ]
 },
 "anim_chat_bg_wait": {
  "anim_type": "wait",
  "duration": "$chat_item_lifetime",
  "next": "@hud.anim_chat_bg_offset"
 },
 "chat_label": {
  "type": "label",
  "layer": 32,
  "text": "#text",
  "size": [
   "100%-0.5px",
   "default"
  ],
  "shadow": true,
  "localize": false,
  "color": "$chat_text_color",
  "font_type": "$chat_font_type",
  "font_scale_factor": "$chat_font_scale_factor",
  "line_padding": "$chat_line_spacing",
  "anims": [
   "@hud.anim_chat_txt_wait"
  ],
  "bindings": [
   {
    "binding_name": "#chat_text",
    "binding_name_override": "#text",
    "binding_type": "collection",
    "binding_collection_name": "chat_text_grid",
    "binding_condition": "once"
   }
  ]
 },
 "root_panel": {
  "type": "panel",
  "$xp_control_offset|default": [
   0,
   -14
  ],
  "controls": [
   {
    "emote_expediate_helpers@hud.emote_expediate_helpers": {}
   },
   {
    "centered_gui_elements@centered_gui_elements": {}
   },
   {
    "centered_gui_elements_at_bottom_middle@centered_gui_elements_at_bottom_middle": {}
   },
   {
    "centered_ridingvr_gui_elements@centered_gui_elements_at_bottom_middle": {
     "anchor_from": "top_middle",
     "anchor_to": "top_middle",
     "offset": [
      0,
      60
     ],
     "size": [
      180,
      50
     ],
     "bindings": [
      {
       "binding_name": "#hud_visible_centered_ridingvr",
       "binding_name_override": "#visible",
       "binding_type": "global"
      }
     ]
    }
   },
   {
    "not_centered_gui_elements@not_centered_gui_elements": {}
   },
   {
    "gamertag_label_for_splitscreen": {
     "type": "label",
     "anchor_from": "top_right",
     "anchor_to": "top_right",
     "color": "$hud_gamertag_color",
     "text_alignment": "right",
     "text": "#gamertag",
     "size": [
      "default",
      "default"
     ],
     "offset": [
      -4,
      4
     ],
     "bindings": [
      {
       "binding_name": "#gamertag"
      }
     ]
    }
   },
   {
    "exp_rend@exp_progress_bar_and_hotbar": {
     "anchor_from": "bottom_middle",
     "anchor_to": "bottom_middle",
     "bindings": [
      {
       "binding_name": "#hotbar_visible_not_centered",
       "binding_name_override": "#visible",
       "binding_type": "global"
      }
     ]
    }
   },
   {
    "exp_rend_resizable@exp_progress_bar_and_hotbar_pocket": {
     "bindings": [
      {
       "binding_name": "#hotbar_visible_not_centered_resizable",
       "binding_name_override": "#visible",
       "binding_type": "global"
      }
     ]
    }
   },
   {
    "hud_tip_text_factory": {
     "type": "factory",
     "control_name": "@hud.hud_tip_text"
    }
   },
   {
    "hud_actionbar_text_area": {
     "type": "panel",
     "factory": {
      "name": "hud_actionbar_text_factory",
      "control_ids": {
       "hud_actionbar_text": "hud_actionbar_text@hud.hud_actionbar_text"
      }
     }
    }
   },
   {
    "hud_title_text_area": {
     "type": "panel",
     "factory": {
      "name": "hud_title_text_factory",
      "control_ids": {
       "hud_title_text": "hud_title_text@hud.hud_title_text"
      }
     }
    }
   },
   {
    "mob_effects_renderer@mob_effects_renderer": {
     "anchor_from": "top_right",
     "anchor_to": "top_right",
     "size": [
      "100%c",
      "74%"
     ],
     "offset": [
      0,
      24
     ],
     "bindings": [
      {
       "binding_name": "(not #on_new_death_screen)",
       "binding_name_override": "#visible"
      }
     ]
    }
   },
   {
    "vignette_rend@vignette_renderer": {
     "visible": false
    }
   },
   {
    "editor_gizmo_rend@editor_gizmo_renderer": {
     "ignored": "(not $is_editor_mode_enabled)"
    }
   },
   {
    "cursor_render@cursor_renderer": {
     "bindings": [
      {
       "binding_name": "(not #on_new_death_screen)",
       "binding_name_override": "#visible"
      }
     ]
    }
   },
   {
    "sidebar@scoreboard.scoreboard_sidebar": {}
   },
   {
    "progress_rend@progress_indicator_renderer": {
     "offset": [
      "50%",
      "50%"
     ]
    }
   },
   {
    "chat_stack": {
     "type": "stack_panel",
     "orientation": "vertical",
     "anchor_from": "top_left",
     "anchor_to": "top_left",
     "size": [
      "40%",
      "100%"
     ],
     "controls": [
      {
       "paper_doll_padding": {
        "type": "panel",
        "size": [
         "100%",
         56
        ],
        "bindings": [
         {
          "binding_name": "#paper_doll_visible",
          "binding_name_override": "#visible"
         }
        ]
       }
      },
      {
       "paper_doll_padding_living_room": {
        "type": "panel",
        "size": [
         "100%",
         46
        ],
        "bindings": [
         {
          "binding_name": "#paper_doll_visible_living_room",
          "binding_name_override": "#visible"
         }
        ]
       }
      },
      {
       "non_centered_gui_padding": {
        "type": "panel",
        "size": [
         "100%",
         32
        ],
        "bindings": [
         {
          "binding_name": "#hud_visible_not_centered",
          "binding_name_override": "#visible",
          "binding_type": "global"
         }
        ]
       }
      },
      {
       "vr_riding_chat_padding": {
        "type": "panel",
        "size": [
         "100%",
         132
        ],
        "visible": "#vr_riding",
        "bindings": [
         {
          "binding_name": "#vr_riding",
          "binding_name_override": "#visible"
         }
        ]
       }
      },
      {
       "vr_chat_padding": {
        "type": "panel",
        "size": [
         "100%",
         30
        ],
        "visible": "#vr_non_riding",
        "bindings": [
         {
          "binding_name": "#vr_non_riding",
          "binding_name_override": "#visible"
         }
        ]
       }
      },
      {
       "chat@hud.chat_panel": {}
      }
     ]
    }
   },
   {
    "boss_health_panel": {
     "type": "stack_panel",
     "orientation": "vertical",
     "size": [
      "100%",
      "100%"
     ],
     "controls": [
      {
       "boss_hud_padding": {
        "type": "panel",
        "size": [
         "100%",
         20
        ],
        "visible": "#boss_hud_padding",
        "bindings": [
         {
          "binding_name": "#boss_hud_padding",
          "binding_name_override": "#visible"
         }
        ]
       }
      },
      {
       "boss_hud_touch_padding": {
        "type": "panel",
        "visible": "#boss_hud_touch_padding",
        "size": [
         "100%",
         "15%"
        ],
        "bindings": [
         {
          "binding_name": "#boss_hud_touch_padding",
          "binding_name_override": "#visible",
          "binding_condition": "always"
         }
        ]
       }
      },
      {
       "boss_panel": {
        "type": "panel",
        "controls": [
         {
          "boss_health_grid@boss_health_grid": {
           "offset": [
            0,
            2
           ]
          }
         }
        ]
       }
      }
     ]
    }
   },
   {
    "position@hud.position_panel": {
     "anchor_from": "bottom_middle",
     "anchor_to": "bottom_middle",
     "offset": [
      0,
      -56
     ]
    }
   },
   {
    "bedrockAntUi_active@common.txtSmooth": {
     "anchor_from": "bottom_left",
     "anchor_to": "bottom_left",
     "text": "hudScreen.bedrockAntUI.active",
     "text_alignment": "left",
     "shadow": true,
     "font_scale_factor": 0.8,
     "alpha": 0.4,
     "color": "grey",
     "size": [
      "100%",
      10
     ]
    }
   },
   {
    "right_survival@hud.right_survival_panel": {}
   },
   {
    "save_icon@hud.auto_save": {}
   }
  ],
  "bindings": [
   {
    "binding_name": "#hud_visible",
    "binding_name_override": "#visible",
    "binding_type": "global"
   },
   {
    "binding_name": "#hud_alpha",
    "binding_name_override": "#alpha",
    "binding_type": "global"
   },
   {
    "binding_name": "#hud_propagate_alpha",
    "binding_name_override": "#propagateAlpha",
    "binding_type": "global"
   }
  ]
 },
 "right_survival_panel@common.vertical": {
  "anchor_from": "bottom_right",
  "anchor_to": "bottom_right",
  "size": [
   18,
   18
  ],
  "offset": [
   -2,
   -2
  ],
  "controls": [
   {
    "compass@hud.compass_icon": {}
   },
   {
    "padding0@common.space": {
     "$size": [
      0,
      2
     ]
    }
   },
   {
    "clock@hud.clock_icon": {}
   },
   {
    "padding1@common.space": {
     "$size": [
      0,
      2
     ]
    }
   }
  ]
 },
 "clock_icon@beacon.item_renderer": {
  "property_bag": {
   "#item_id_aux": 22740992
  }
 },
 "compass_icon@beacon.item_renderer": {
  "property_bag": {
   "#item_id_aux": 22609920
  }
 },
 "button_hide_hud@settings_common.action_button": {
  "prevent_touch_input": true,
  "$pressed_button_name": "button.hide_gui_all",
  "$button_text": "hudScreen.button.hideHud",
  "$default_button_texture": "textures/ui/black_content",
  "$hover_button_texture": "textures/ui/black_content",
  "$pressed_button_texture": "textures/ui/black_content",
  "$default_text_color": "white",
  "$hover_text_color": "white",
  "$pressed_text_color": "$title_text_color",
  "$focus_override_right": "FOCUS_OVERRIDE_STOP"
 },
 "hud_screen@common.base_screen": {
  "$screen_content": "hud.hud_screen_content",
  "should_steal_mouse": true,
  "send_telemetry": false,
  "is_showing_menu": false
 },
 "hud_screen_content": {
  "type": "panel",
  "size": "$hud_size",
  "$hud_size": [
   "100%-4px",
   "100%-4px"
  ],
  "$hud_player_rend_desktop_offset|default": [
   16,
   16
  ],
  "variables": [
   {
    "requires": "($is_holographic and not $is_living_room_mode)",
    "$hud_player_rend_desktop_offset": [
     100,
     50
    ]
   },
   {
    "requires": "($compress_hud_width)",
    "$hud_size": [
     "90%",
     "100%"
    ]
   }
  ],
  "controls": [
   {
    "hide_hud@hud.button_hide_hud": {
     "ignored": "$win10_edition",
     "anchor_from": "bottom_left",
     "anchor_to": "bottom_left",
     "offset": [
      2,
      -2
     ],
     "size": [
      68,
      16
     ]
    }
   },
   {
    "root@root_panel": {}
   },
   {
    "editor_gizmo_renderer@editor_gizmo_renderer": {
     "ignored": "(not $is_editor_mode_enabled)"
    }
   },
   {
    "camera@camera_renderer": {}
   }
  ]
 }
}