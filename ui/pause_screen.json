{
  "namespace": "pause",
  "achievements_icon@pause.img": {
    "texture": "textures/ui/achievements_icon",
    "size": [
      16,
      16
    ]
  },
  "feedback_icon@pause.img": {
    "texture": "textures/ui/feedback_icon",
    "size": [
      16,
      16
    ]
  },
  "screenshot_icon@pause.img": {
    "texture": "textures/ui/screenshot_icon",
    "size": [
      16,
      16
    ]
  },
  "button_continue@button": {
    "$pressed_button_name": "button.menu_continue",
    "$button_text": "pause.continue"
  },
  "button_settings@button": {
    "$pressed_button_name": "button.menu_settings",
    "$button_text": "pause.settings"
  },
  "store_button@pause.pause_button_template": {
    "ignored": "$is_secondary_client",
    "$pressed_button_name": "button.menu_store",
    "$button_text": "$store_button_text",
    "$font_scale_factor": 1.2,
    "bindings": [
      {
        "binding_name": "#store_button_visible",
        "binding_name_override": "#visible",
        "binding_type": "global"
      }
    ]
  },
  "button_how_to_play@button": {
    "$pressed_button_name": "button.menu_how_to_play",
    "$button_text": "pause.howToPlay"
  },
  "button_profile@button": {
    "$pressed_button_name": "button.to_profile_or_skins_screen",
    "$button_text": "pause.profile",
    "$button_tts_header": "accessibility.start.profile",
    "$focus_id": "profile_button",
    "bindings": [
      {
        "binding_name": "#can_change_skin",
        "binding_name_override": "#enabled"
      }
    ]
  },
  "skin_panel_interior": {
    "type": "stack_panel",
    "size": [
      "100%",
      "100%c"
    ],
    "orientation": "vertical",
    "controls": [
      {
        "offset_panel": {
          "type": "panel",
          "size": [
            "100%",
            "100%c"
          ],
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle",
          "controls": [
            {
              "viewer_panel@pause.skin_viewer_panel": {
                "tts_ignore_count": true
              }
            }
          ]
        }
      }
    ]
  },
  "button_quit@button": {
    "$pressed_button_name": "button.menu_quit",
    "$button_text": "pause.quit",
    "variables": [
      {
        "requires": "$is_secondary_client",
        "$button_text": "pause.quit",
        "$font_type": "MinecraftTen",
        "$font_scale_factor": 1.2
      }
    ]
  },
  "button_achievements@icon": {
    "$pressed_button_name": "button.menu_achievements",
    "$button_content": "pause.achievements_icon",
    "bindings": [
      {
        "binding_name": "#sign_out_visible",
        "binding_name_override": "#visible"
      }
    ]
  },
  "button_feedback@icon": {
    "$pressed_button_name": "button.feedback_button_pressed",
    "$button_content": "pause.feedback_icon"
  },
  "button_screenshot@icon": {
    "$pressed_button_name": "button.screenshot",
    "$button_content": "pause.screenshot_icon",
    "sound_pitch": 1.0,
    "sound_name": "random.screenshot",
    "sound_volume": 1.0
  },
  "pause_screen@common.base_screen": {
    "load_screen_immediately": true,
    "$screen_content": "pause pause_screen_content",
    "$screen_bg_content": "common.screen_background"
  },
  "icon@common_buttons.light_content_button": {},
  "button@common_buttons.light_text_button": {},
  "img": {
    "type": "image",
    "layer": 2
  },
  "pause_screen_content": {
    "type": "panel",
    "size": [
      "100%",
      "100%"
    ],
    "controls": [
      {
        "title@common_art.pause_logo_panel": {
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "offset": [
            -70,
            8
          ]
        }
      },
      {
        "player@pause.info_panel_content_list": {
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "size": [
            "26% + 18px",
            "96% + 18px"
          ]
        }
      },
      {
        "skin@pause.skin_panel": {
          "anchor_from": "center",
          "anchor_to": "center",
          "size": [
            "40%",
            "100%"
          ],
          "max_size": [
            "100%",
            "100%"
          ],
          "offset": [
            58,
            22
          ]
        }
      },
      {
        "main_panel": {
          "type": "stack_panel",
          "anchor_from": "center",
          "anchor_to": "center",
          "orientation": "vertical",
          "size": [
            150,
            26
          ],
          "offset": [
            -65,
            -14
          ],
          "controls": [
            {
              "continue@pause.button_continue": {}
            },
            {
              "horizontal_panel": {
                "type": "stack_panel",
                "orientation": "horizontal",
                "size": [
                  75,
                  26
                ],
                "controls": [
                  {
                    "settings@pause.button_settings": {}
                  },
                  {
                    "profile@pause.button_profile": {}
                  }
                ]
              }
            },
            {
              "store@pause.store_button_panel": {}
            },
            {
              "quit@pause.button_quit": {}
            }
          ]
        }
      },
      {
        "bottom_panel": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "size": [
            26,
            26
          ],
          "offset": [
            5,
            -5
          ],
          "controls": [
            {
              "achievement@pause.button_achievements": {}
            },
            {
              "feedback@pause.button_feedback": {}
            },
            {
              "screenshot@pause.button_screenshot": {}
            }
          ]
        }
      }
    ]
  }
}