{
 "namespace": "screenshot",
 "screenshot_flash_panel@common.img": {
  "texture": "textures/ui/White",
  "alpha": "@screenshot.anim_screenshot_flash",
  "layer": 8,
  "fill": true,
  "size": [
   "100%",
   "100%"
  ]
 },
 "anim_screenshot_flash": {
  "anim_type": "alpha",
  "easing": "in_expo",
  "duration": 1,
  "from": 1.0,
  "to": 0
 },
 "button_share@common.green_content_button": {
  "$pressed_button_name": "button.share_screenshot",
  "$button_content": "screenshot.share_content_panel",
  "enabled": "#enabled",
  "bindings": [
   {
    "binding_name": "#player_offline",
    "binding_name_override": "#enabled"
   }
  ]
 },
 "share_icon@common.img": {
  "texture": "textures/hbui/share",
  "layer": 1,
  "size": [
   12,
   16
  ]
 },
 "share_content_panel": {
  "type": "panel",
  "anchor_from": "top_left",
  "anchor_to": "top_left",
  "size": [
   "100%-2px",
   "100%-2px"
  ],
  "controls": [
   {
    "stack_panel": {
     "type": "stack_panel",
     "orientation": "horizontal",
     "controls": [
      {
       "icon_panel": {
        "type": "panel",
        "size": [
         "100%c",
         "100%"
        ],
        "controls": [
         {
          "icon@screenshot.share_icon": {}
         }
        ]
       }
      },
      {
       "offset": {
        "type": "panel",
        "size": [
         4,
         "100%"
        ]
       }
      },
      {
       "label_panel": {
        "type": "panel",
        "size": [
         "100%c",
         "100%"
        ],
        "controls": [
         {
          "label@common.txtTen": {
           "text": "screenshotScreen.share",
           "font_scale_factor": 1.2,
           "color": "$screenshot_text_color",
           "layer": 1,
           "controls": [
            {
             "shadow@common.txtTen": {
              "text": "screenshotScreen.share",
              "alpha": "$shadow_alpha_label",
              "$shadow_alpha_label": 0.4,
              "font_scale_factor": 1.2,
              "color": "black",
              "layer": -1,
              "offset": [
               1,
               1
              ]
             }
            }
           ]
          }
         }
        ]
       }
      }
     ]
    }
   }
  ]
 },
 "button_cancel@common.red_content_button": {
  "$pressed_button_name": "button.menu_exit",
  "$button_content": "screenshot.cancel_content_panel",
  "bindings": [
   {
    "binding_name": "(not #is_using_gamepad)",
    "binding_name_override": "#visible"
   }
  ]
 },
 "cancel_icon@common.img": {
  "texture": "textures/hbui/close12x",
  "layer": 1,
  "size": [
   12,
   12
  ]
 },
 "cancel_content_panel": {
  "type": "panel",
  "anchor_from": "top_left",
  "anchor_to": "top_left",
  "size": [
   "100%-2px",
   "100%-2px"
  ],
  "controls": [
   {
    "stack_panel": {
     "type": "stack_panel",
     "orientation": "horizontal",
     "controls": [
      {
       "icon_panel": {
        "type": "panel",
        "size": [
         "100%c",
         "100%"
        ],
        "controls": [
         {
          "icon@screenshot.cancel_icon": {}
         }
        ]
       }
      },
      {
       "offset": {
        "type": "panel",
        "size": [
         4,
         "100%"
        ]
       }
      },
      {
       "label_panel": {
        "type": "panel",
        "size": [
         "100%c",
         "100%"
        ],
        "controls": [
         {
          "label@common.txtTen": {
           "text": "screenshotScreen.cancel",
           "font_scale_factor": 1.2,
           "color": "$screenshot_text_color",
           "layer": 1,
           "controls": [
            {
             "shadow@common.txtTen": {
              "text": "screenshotScreen.cancel",
              "alpha": "$shadow_alpha_label",
              "font_scale_factor": 1.2,
              "color": "black",
              "layer": -1,
              "offset": [
               1,
               1
              ]
             }
            }
           ]
          }
         }
        ]
       }
      }
     ]
    }
   }
  ]
 },
 "share_text_box@common.text_edit_box": {
  "$place_holder_text": "screenshotScreen.whatsUp.textBox",
  "$place_holder_text_color": "grey",
  "$text_box_text_color": "grey",
  "$text_box_name": "#caption_text_box",
  "max_length": 156,
  "bindings": [
   {
    "binding_name": "#caption_text_box"
   }
  ]
 },
 "share_text_box_panel@common.horizontal": {
  "size": [
   "100%",
   "100%c"
  ],
  "controls": [
   {
    "padding0@common.panel": {
     "size": [
      "fill",
      0
     ]
    }
   },
   {
    "text_box@screenshot.share_text_box": {
     "anchor_from": "center",
     "anchor_to": "center",
     "size": [
      226,
      16
     ]
    }
   },
   {
    "padding1@common.panel": {
     "size": [
      "fill",
      0
     ]
    }
   }
  ]
 },
 "screenshot_title_label@common.txtTen": {
  "text": "screenshotScreen.postScreenshot",
  "text_alignment": "center",
  "font_scale_factor": 1.6,
  "color": "$screenshot_text_color",
  "layer": 1,
  "size": [
   "100%",
   "default"
  ],
  "controls": [
   {
    "shadow@common.txtTen": {
     "text": "screenshotScreen.postScreenshot",
     "color": "black",
     "alpha": "$shadow_alpha_label",
     "font_scale_factor": 1.6,
     "layer": -1,
     "offset": [
      1.4,
      1.4
     ]
    }
   }
  ]
 },
 "saved_capture_path_panel@common.horizontal": {
  "ignored": "((not $win10_edition) and (not $is_ios) and (not $is_android))",
  "anchor_from": "center",
  "anchor_to": "center",
  "size": [
   "100%",
   "100%c"
  ],
  "controls": [
   {
    "padding0@common.panel": {
     "size": [
      "fill",
      0
     ]
    }
   },
   {
    "saved_location_image@common.img": {
     "anchor_from": "center",
     "anchor_to": "center",
     "texture": "textures/ui/black_content",
     "layer": 2,
     "alpha": 0.6,
     "size": [
      "100%c+8px",
      "100%c+4px"
     ],
     "controls": [
      {
       "saved_capture_text@common.txt": {
        "text": "#text",
        "text_alignment": "center",
        "hide_hyphen": true,
        "color": "grey",
        "alpha": 0.8,
        "font_type": "MinecraftSeven",
        "layer": 2,
        "size": [
         236,
         "default"
        ],
        "bindings": [
         {
          "binding_name": "#screenshot_texture",
          "binding_name_override": "#text"
         }
        ]
       }
      }
     ]
    }
   },
   {
    "padding1@common.panel": {
     "size": [
      "fill",
      0
     ]
    }
   }
  ]
 },
 "screenshot_image_panel@common.horizontal": {
  "anchor_from": "center",
  "anchor_to": "center",
  "size": [
   "100%",
   "100%c"
  ],
  "controls": [
   {
    "padding0@common.panel": {
     "size": [
      "fill",
      0
     ]
    }
   },
   {
    "screenshot_image_content@common.img": {
     "anchor_from": "center",
     "anchor_to": "center",
     "texture": "textures/ui/image_background",
     "layer": 1,
     "size": [
      192,
      112
     ],
     "controls": [
      {
       "screenshot_image@common.img": {
        "texture": "#texture",
        "layer": 3,
        "size": [
         "100%-8px",
         "100%"
        ],
        "bindings": [
         {
          "binding_name": "#screenshot_texture",
          "binding_name_override": "#texture"
         },
         {
          "binding_name": "#screenshot_texture_source",
          "binding_name_override": "#texture_file_system"
         }
        ]
       }
      }
     ]
    }
   },
   {
    "padding1@common.panel": {
     "size": [
      "fill",
      0
     ]
    }
   }
  ]
 },
 "main_button_panel@common.horizontal": {
  "anchor_from": "bottom_middle",
  "anchor_to": "bottom_middle",
  "size": [
   "100%",
   26
  ],
  "controls": [
   {
    "padding0@common.panel": {
     "size": [
      120,
      "100%"
     ]
    }
   },
   {
    "share@screenshot.button_share": {
     "size": [
      "20%",
      "100%"
     ]
    }
   },
   {
    "fill0@common.panel": {
     "size": [
      "fill",
      "100%"
     ]
    }
   },
   {
    "cancel@screenshot.button_cancel": {
     "size": [
      "20%",
      "100%"
     ]
    }
   },
   {
    "padding1@common.panel": {
     "size": [
      120,
      "100%"
     ]
    }
   }
  ]
 },
 "main_content_panel@common.vertical": {
  "anchor_from": "center",
  "anchor_to": "center",
  "size": [
   "100%",
   "100%c"
  ],
  "offset": [
   0,
   2
  ],
  "controls": [
   {
    "screenshot_title@screenshot.screenshot_title_label": {}
   },
   {
    "padding0@common.panel": {
     "size": [
      "100%",
      6
     ]
    }
   },
   {
    "screenshot_image@screenshot.screenshot_image_panel": {}
   },
   {
    "padding1@common.panel": {
     "size": [
      "100%",
      2
     ]
    }
   },
   {
    "share_text_box@screenshot.share_text_box_panel": {}
   },
   {
    "padding2@common.panel": {
     "size": [
      "100%",
      4
     ]
    }
   },
   {
    "capture_path@screenshot.saved_capture_path_panel": {}
   },
   {
    "padding3@common.panel": {
     "size": [
      "100%",
      6
     ]
    }
   },
   {
    "main_button@screenshot.main_button_panel": {}
   }
  ]
 },
 "gamepad_helpers@common.gamepad_helpers_a_and_b": {
  "layer": 0,
  "anchor_from": "bottom_middle",
  "anchor_to": "bottom_middle",
  "offset": [
   0,
   "30px"
  ]
 },
 "screenshot_screen_content": {
  "type": "panel",
  "size": [
   "100%",
   "100%"
  ],
  "controls": [
   {
    "main_content@main_content_panel": {}
   },
   {
    "screenshot_flash@screenshot_flash_panel": {}
   },
   {
    "gamepad_panel@gamepad_helpers": {}
   }
  ]
 },
 "screenshot_screen@common.base_screen": {
  "$screen_content": "screenshot.screenshot_screen_content",
  "$screen_bg_content": "common.screen_background",
  "$screen_bg_alpha": 0.2,
  "button_mappings": [
   {
    "from_button_id": "button.menu_exit",
    "to_button_id": "button.menu_cancel",
    "mapping_type": "global"
   },
   {
    "from_button_id": "button.menu_cancel",
    "to_button_id": "button.menu_exit",
    "mapping_type": "global"
   },
   {
    "from_button_id": "button.menu_up",
    "to_button_id": "button.menu_up",
    "mapping_type": "global",
    "scope": "view"
   },
   {
    "from_button_id": "button.menu_down",
    "to_button_id": "button.menu_down",
    "mapping_type": "global",
    "scope": "view"
   },
   {
    "from_button_id": "button.menu_left",
    "to_button_id": "button.menu_left",
    "mapping_type": "global",
    "scope": "view"
   },
   {
    "from_button_id": "button.menu_right",
    "to_button_id": "button.menu_right",
    "mapping_type": "global",
    "scope": "view"
   }
  ]
 }
}