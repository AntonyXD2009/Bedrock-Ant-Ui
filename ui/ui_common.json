{
 "namespace": "common",
 "selected_item_details": {
  "type": "panel",
  "anchor_to": "bottom_middle",
  "anchor_from": "bottom_middle",
  "size": [
   "100%c",
   "100%c"
  ],
  "offset": "$offset",
  "layer": 30,
  "$offset|default": [
   0,
   "-8px"
  ],
  "$alpha_anim|default": "@common.anim_item_details_alpha",
  "controls": [
   {
    "item_panel_image": {
     "type": "image",
     "layer": 30,
     "visible": "#visible",
     "alpha": "$alpha_anim",
     "texture": "textures/ui/purpleBorder",
     "size": [
      "100%c+8px",
      "100%c+8px"
     ],
     "controls": [
      {
       "item_text_label": {
        "type": "label",
        "shadow": true,
        "layer": 1,
        "alpha": "$alpha_anim",
        "text": "#selected_hover_text",
        "color": "$item_hover_text_color",
        "enable_profanity_filter": false
       }
      }
     ]
    }
   }
  ],
  "bindings": [
   {
    "binding_name": "#recipe_details_label",
    "binding_name_override": "#visible"
   }
  ]
 },
 "light_ui_toggle": {
  "type": "panel",
  "$toggle_view_binding_name|default": "this_toggle",
  "$template_toggle|default": "common_toggles.light_template_toggle",
  "controls": [
   {
    "$toggle_view_binding_name@$template_toggle": {}
   }
  ]
 },
 "light_content_toggle@common_toggles.light_ui_toggle": {
  "$button_type_panel": "common_buttons.button_content_panel",
  "$button_content|default": "common_toggles.toggle_content_panel",
  "$button_offset|default": [
   0,
   0
  ]
 },
 "option_toggle_0@settings_common.option_toggle": {
  "$toggle_name": "see_more_toggle",
  "$option_binding_name": "#see_more_toggle",
  "$option_label": "Toggle Label",
  "$control_name": "general_section.advanced_video_options_toggle"
 },
 "bindings": [
  {
   "binding_type": "view",
   "source_control_name": "general_section.advanced_video_options_toggle",
   "source_property_name": "#toggle_state",
   "target_property_name": "#visible"
  }
 ],
 // use "$header_title_text" for the title visible //
 "new_screen_header_panel": {
  "type": "stack_panel",
  "anchor_from": "top_left",
  "anchor_to": "top_left",
  "controls": [
   {
    "top_bar_image@common.new_top_bar": {
     "$screen_header_label": "$header_title_text",
     "$header_title_text|default": "Header Empty Title",
     "size": [
      "100%",
      30
     ],
     "controls": [
      {
       "content_panel": {
        "type": "panel",
        "size": [
         "100%",
         16
        ],
        "controls": [
         {
          "button_arrow_back@common.button": {
           "$pressed_button_name": "button.menu_exit",
           "visible": "$back_button_visible",
           "$back_button_visible|default": true,
           "focus_enabled": false,
           "anchor_from": "left_middle",
           "anchor_to": "left_middle",
           "size": [
            "100%c",
            "100%c"
           ],
           "offset": [
            2,
            -2
           ],
           "$arrow_size": [
            24,
            24
           ],
           "bindings": [
            {
             "binding_name": "#close_button_visible",
             "binding_name_override": "#visible"
            }
           ],
           "controls": [
            {
             "default@common.arrow_left_black": {
              "size": "$arrow_size"
             }
            },
            {
             "hover@common.arrow_left_black_hover": {
              "size": "$arrow_size"
             }
            },
            {
             "pressed@common.arrow_left_black": {
              "size": "$arrow_size"
             }
            }
           ]
          }
         },
         {
          "title_panel@common.txt": {
           "anchor_from": "center",
           "anchor_to": "center",
           "text": "$screen_header_label",
           "text_alignment": "center",
           "layer": 3,
           "font_type": "MinecraftTen",
           "font_scale_factor": 1.4,
           "color": "black",
           "size": [
            "100%",
            "100%"
           ],
           "offset": [
            0,
            -2.6
           ]
          }
         }
        ]
       }
      }
     ]
    }
   }
  ]
 },
